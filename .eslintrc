{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "require-jsdoc": "off",
    "operator-linebreak": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
